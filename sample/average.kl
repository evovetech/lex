
# sum
def sum(x y)
    x + y;

# average
def average(x y)
    sum(x, y) * 0.5;

# min
def min(x y)
    if (x < y) then
        x
    else
        y

# max
def max(x y)
    if (x < y) then
        y
    else
        x

# Compute the x'th fibonacci number.
def fib(x)
  if x < 3 then
    1
  else
    fib(x-1) + fib(x-2)

# call externals
extern sin(arg);
extern cos(arg);
extern atan2(arg1 arg2);

def callAtan2(x y)
    atan2(sin(x), cos(y))
